  "homepage": "https://github.com/agenticui/agentic-ui#readme",
  "bugs": {
    "url": "https://github.com/agenticui/agentic-ui/issues"
  },
  "type": "module",
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
